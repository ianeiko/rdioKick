{
  "name": "rdioKick",
  "version": "1.0",
  "manifest_version": 2,

  "description": "Shows concert dates on a band's Rdio profile",

  "permissions": [
    "http://www.rdio.com/",
    "http://api.jambase.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://www.rdio.com/*"],
      "js": [
        "mustache.js",
        "js/rdiokick.js"
      ]
    }
  ],
  "web_accessible_resources": ["templates.html"]
}
