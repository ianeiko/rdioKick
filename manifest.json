{
  "name": "rdioKick",
  "version": "1.0",
  "manifest_version": 2,

  "description": "Shows concert dates on a band's Rdio profile",

  "browser_action": {
    "name": "rdioKick"
    // "icons": ["icon.png"]
  },

  "permissions": [
    "http://www.rdio.com/*",
    "http://api.jambase.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://www.rdio.com/artist/*"],
      "js": [
        "jquery.js",
        "xml2json.js",
        "sugar.js",
        "js/rdiokick.js"
      ],
      "run_at": "document_end"
    }
  ]
}
